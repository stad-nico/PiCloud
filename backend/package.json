{
	"name": "backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"openapi-generate": "SET NODE_ENV=test&& ts-node src/openapi.ts",
		"mikro-orm:dev": "SET NODE_ENV=dev&& npx mikro-orm",
		"mikro-orm:test": "SET NODE_ENV=test&& npx mikro-orm",
		"mikro-orm:prod": "SET NODE_ENV=prod&& npx mikro-orm",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start:dev": "SET NODE_ENV=dev&& nest start --watch",
		"start:debug": "SET NODE_ENV=dev&& nest start --debug --watch",
		"start:prod": "SET NODE_ENV=prod&& nest start",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:unit": "jest --roots test/unit",
		"test:unit:cov": "jest --roots test/unit --coverage",
		"test:integration": "jest --roots test/integration",
		"test:integration:cov": "jest --roots test/integration --coverage",
		"test:e2e": "jest --roots test/e2e",
		"test:e2e:cov": "jest --roots test/e2e --coverage"
	},
	"dependencies": {
		"@mikro-orm/cli": "^6.4.4",
		"@mikro-orm/core": "^6.4.4",
		"@mikro-orm/mariadb": "^6.4.4",
		"@mikro-orm/migrations": "^6.4.4",
		"@mikro-orm/nestjs": "^6.0.2",
		"@mikro-orm/seeder": "^6.4.4",
		"@nestjs/common": "^10.3.8",
		"@nestjs/config": "^3.2.2",
		"@nestjs/core": "^10.3.8",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/platform-express": "^10.3.8",
		"@nestjs/swagger": "^7.3.1",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dotenv": "^16.4.5",
		"jszip": "^3.10.1",
		"mariadb": "^3.4.0",
		"reflect-metadata": "^0.2.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.11",
		"@nestjs/cli": "^10.3.2",
		"@nestjs/testing": "^10.3.8",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.12.10",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"swc-loader": "^0.2.6",
		"ts-jest": "^29.1.2",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"mikro-orm": {
		"useTsNode": true,
		"configPaths": [
			"src/config/mikro-orm.config.ts",
			"dist/config/mikro-orm.config.js"
		]
	}
}
